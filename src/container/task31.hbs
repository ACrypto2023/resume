<main class='ms-5'>
 {{#with navigation }}
 {{#each links}}
 <a href="{{href}}" class='btn text-primary'>{{text}}</a>
 {{/each}}
  
   <a type='button'  class='btn btn-primary' href="{{button.href}}" target="_blank">{{button.text}}</a>
   {{/with}}
   <br>
   <hr>
   
   <h1 class="mt-5">{{header.title}}</h1>
   
   <h6 class='text-muted mt-4'>{{header.description}}</h6>
   <a type='button' class='btn btn-primary my-4' href="{{header.button.link}}" target="_blank">{{header.button.text}}</a>
   
   <hr>
   
              
   {{#with city}}
   <h2 class="mt-4">{{title}}</h2>
  
      <div class="row">
      {{#each cards}}
        
            <div class="col-6">
              <div class="card mt-4 my-2">
                  <img src="{{picture.src}}" class="card-img-top" alt="{{picture.alt}}">
                  <div class="card-body">
                    <h5 class="card-title">{{title}}</h5>
                    <p class="card-text">{{description}}</p>
                    <a href={{button.href}} class="btn btn-primary">{{button.text}}</a><br>
                  </div>
               </div>
            </div>
        
      {{/each}}
      </div> 
{{/with}}



   <h2 class="my-4">{{winner.title}}</h2>
    <table class="table mb-5">
    <thead>
      <tr>
        {{#each winner.table.head}}
        <th>
          {{this}}
        </th>
        {{/each}}
        </tr>
    </thead>
    <tbody>
        {{#each winner.table.user}}
      <tr>
        <th>
          {{place}}
        </th>
        <th>
          {{name}}
        </th>
        <th>
          {{surname}}
        </th>
        <th>
          {{nickname}}
        </th>
        
      </tr>
      {{/each}}
      
      

    </tbody>
    </table>
   
   <hr>
   
   <h2 class="my-4">{{gallery.title}}</h2>
   
   <div class="row">
         {{#each gallery.cards}}
          <div class="col-4">
                <div class='card'>
                        
                                <ul class="card-title list-group text-bg-light">{{name}}
                                                {{#each items}}
                                                <li class="list-group-item">{{this}}</li>
                                                {{/each}}
                                       
                                </ul>
                       
                </div>
            </div>
          {{/each}}
     </div>
    
<div class='display-3 mt-4'><b>{{relaxion.title}}</b></div><br>
<div class="row mb-4">
    {{#each relaxion.cards}}
      <div class="col-5 ">
        <div class='card text-bg-secondary ' >
            <ul class=" list-group text-bg-secondary">
                    <li class=" card-title list-group-item text-bg-secondary">{{header}}</li>
                    <li class="list-group-item text-bg-secondary">
                      <h5>{{title}}</h5>
                     
                      {{description}}
                    </li>
                    
                                  
            </ul>
        </div><br>
      </div>
    {{/each}}

    </div>

</div>
<hr><br>
<div class="row">
      {{#each rest.cards}}
        
          <div class="col-6">
            

                <div class="card text-center ">
                  <div class="card-header text-bg-light text-muted">
                   <h6> {{header }}</h6>
                  </div>
                  <div class="card-body">
                    <h5 class="card-title text-muted">{{title}}</h5>
                    <p class="card-text">{{description}}</p>
                    <a href="#" class="btn btn-primary">{{button}}</a>
                  </div>
                  <div class="card-footer text-muted">
                   <h6> {{footer}}</h6>
                  </div>
                </div>

          </div>
        
      {{/each}}
      </div>  
    

<div class='display-3 mt-4'><b>{{proposition.title}}</b></div><br>
<div class="row mb-4">
   {{#each proposition.card}}
    <div class="col-5 ">
      <div class='card text-bg-warning mb-4' >
          <ul class=" list-group ">
                  <li class=" card-title list-group-item text-bg-warning">{{header}}</li>
                  <li class="list-group-item text-bg-warning">
                    <h5>{{title}}</h5>
                    <br>
                    {{description}}
                            
           </ul>
      </div>
      </div>
    {{/each}}

    </div>

</div>
<hr>
 <h5 class='mt-4'>{{footer.title}}</h5><br>
  <div class="row">
    <div class="col-4 ">
            <div class="card card border-0">
            {{#each footer.navigation}}
              
              <a  class='btn btn-outline-primary' href={{href}} target="_blank">{{text}}</a><br>
              
              
              {{/each}}
              </div>
      </div>
      <div class="col-4 ">
            <div class="card card border-0">
            {{#each footer.socialMedia}}
              
              <a  class='btn btn-outline-dark' href={{link}} target="_blank">{{name}}</a><br>
              
              
              {{/each}}
              </div>
      </div>
      <div class="col-4 ">
            <div class="card card border-0">
            {{#each footer.partners}}
              
              <a  class='btn btn-outline-info' href={{link}} target="_blank">{{name}}</a><br>
              
              
              {{/each}}
              </div>
      </div>
  </div>
  
  
  
  
  
  




  
</main>